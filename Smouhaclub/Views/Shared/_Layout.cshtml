    <script src="@SettingHelper.GetPublishName()/assets/js/jquery-3.7.1.min.js" asp-append-version="true"></script>
    <script src="@SettingHelper.GetPublishName()/assets/js/jquery-ui.min.js" asp-append-version="true"></script>
    <script src="@SettingHelper.GetPublishName()/assets/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="@SettingHelper.GetPublishName()/assets/js/owl.carousel.min.js" asp-append-version="true"></script>
    <script src="@SettingHelper.GetPublishName()/assets/js/select2.min.js" asp-append-version="true"></script>
    <script src="@SettingHelper.GetPublishName()/assets/js/sweetalert2.all.min.js" asp-append-version="true"></script>
    <script src="@SettingHelper.GetPublishName()/assets/js/script.js" asp-append-version="true"></script>
